@model HumanResourcesMaster
@{
}
<form asp-controller="HRSearch" asp-action="Edit" method="post">
    <div class="form-group">
        <label for="Id">Id</label>
        <input for="Id" id="Id" name="Id" class="easyui-textbox" style="width:100%;height:30px" value="@Model.Id">
    </div>
    <div class="form-group">
        <label for="Name">Name</label>
        <input for="Name" id="Name" name="Name" class="easyui-textbox" style="width:100%;height:30px" value="@Model.Name"
               autofocus>
    </div>
    <div class="form-group">
        <label for="Gender">Gender</label>
        <input for="Gender" id="Gender" name="Gender" class="easyui-textbox" style="width:100%;height:30px" value="@Model.Gender">
    </div>
    <div class="form-group">
        <label for="Email">Email</label>
        <input for="Email" id="Email" name="Email" class="easyui-textbox" style="width:100%;height:30px" value="@Model.Email">
    </div>
    <div class="form-group">
        <label for="Phone">Phone</label>
        <input for="Phone" id="Phone" name="Phone" class="easyui-textbox" style="width:100%;height:30px" value="@Model.Phone">
    </div>
    <div class="form-group">
        <label for="CellPhone">CellPhone</label>
        <input for="CellPhone" id="CellPhone" name="CellPhone" class="easyui-textbox" style="width:100%;height:30px" value="@Model.CellPhone">
    </div>
    <div class="form-group">
        <label for="WorkRegion">WorkRegion</label>
        <input for="WorkRegion" id="WorkRegion" name="WorkRegion" class="easyui-textbox" style="width:100%;height:30px" value="@Model.WorkRegion">
    </div>
    <div class="form-group">
        <label for="Ability">Ability</label>
        <input for="Ability" id="Ability" name="Ability" class="easyui-textbox" style="width:100%;height:30px" value="@Model.Ability">
    </div>
    <div class="form-group">
        <label for="Status">Status</label>
        <input for="Status" id="Status" name="Status" class="easyui-textbox" style="width:100%;height:30px" value="@Model.Status">
    </div>

    <button type="submit" class="btn btn-primary btn-block mt-4">Edit</button>
</form>