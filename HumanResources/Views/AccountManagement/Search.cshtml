@model AccountManagementSearchViewModel

<div class="container">
    <div class="row">
        <div class="col-2">
            <h3>@Model.Title</h3>

        </div>
        <div class="col-2">
            <div style="align-items:end">
                <form asp-controller="AccountManagement" asp-action="AddAccount" method="get">

                    <button type="submit" class="bun btn-primary">新增資料</button>
                </form>
            </div>
        </div>
    </div>
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Edit</th>
                <th scope="col">Del</th>
                <th scope="col">Name</th>
                <th scope="col">Gender</th>
                <th scope="col">Email</th>
                <th scope="col">Phone</th>
                <th scope="col">CellPhone</th>
            </tr>
        </thead>
        <tbody>
            @foreach (SystemUserData data in Model.UserDatas)
            {
                <tr>
                    <td>
                        @* <a type="submit" class="btn btn-primary" asp-action="Edit" asp-route-id="@data.Id">Edit</a> *@
                    </td>
                    <td>
                        @* <a type="submit" class="btn btn-danger" asp-action="Delete" asp-route-id="@data.Id">Del</a> *@
                    </td>
                    <td>@data.UserName</td>
                    <td>@data.UserAccount</td>
                    <td>@data.UserEmail</td>
                    <td>@data.CreateTime</td>
                    <td>@data.LastModifiedBy</td>
                    <td>@data.LastModifiedTime</td>
                </tr>
            }
        </tbody>
    </table>
</div>