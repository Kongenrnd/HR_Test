@model AddAccountViewModel
@{
}
<form asp-controller="AccountManagement" asp-action="AddAccount" method="post">
    <div class="form-group" >
        <label asp-for="UserName"></label>
        <input asp-for="UserName" id="UserName" name="UserName" class="easyui-textbox" style="width:100%;height:30px" value="@Model.UserName">
    </div>
    <div class="form-group">
        <label asp-for="UserEmail"></label>
        <input asp-for="UserEmail" id="UserEmail" name="UserEmail" class="easyui-textbox" style="width:100%;height:30px" value="@Model.UserEmail"
               autofocus>
    </div>
    <div class="form-group" >
        <label asp-for="UserAccount"></label>
        <input asp-for="UserAccount" id="UserAccount" name="UserAccount" class="easyui-textbox" style="width:100%;height:30px" value="@Model.UserAccount">
    </div>
    <div class="form-group">
        <label asp-for="UserPassword"></label>
        <input asp-for="UserPassword" id="UserPassword" name="UserPassword" class="easyui-textbox" style="width:100%;height:30px" value="@Model.UserPassword">
    </div>
    <div class="form-group">
        <label asp-for="UserAuth"></label>
        <input asp-for="UserAuth" id="UserAuth" name="UserAuth" class="easyui-textbox" style="width:100%;height:30px" value="@Model.UserAuth">
    </div>
    <div class="container">
        <div class="row">

            <div class="col-1">
                <button type="submit" class="btn btn-primary btn-block mt-4">ADD</button>
            </div>
            <div class="col-1" style="margin: 24px;">
                <a class="btn btn-primary" asp-controller="AccountManagement" asp-action="AddAccount">返回</a>
            </div>
        </div>
    </div>
</form>
