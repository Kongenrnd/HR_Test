@model HumanResourcesMaster
@{
}
<form asp-controller="HRSearch" asp-action="Edit" method="post">
    <div class="form-group" hidden>
        <label asp-for="Id">Id</label>
        <input asp-for="Id" id="Id" name="Id" class="easyui-textbox" style="width:100%;height:30px" value="@Model.Id">
    </div>
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" id="Name" name="Name" class="easyui-textbox" style="width:100%;height:30px" value="@Model.Name"
               autofocus>
    </div>
    <div class="form-group">
        <label asp-for="Gender"></label>
        <input asp-for="Gender" id="Gender" name="Gender" class="easyui-textbox" style="width:100%;height:30px" value="@Model.Gender">
    </div>
    <div class="form-group">
        <label asp-for="Email"></label>
        <input asp-for="Email" id="Email" name="Email" class="easyui-textbox" style="width:100%;height:30px" value="@Model.Email">
    </div>
    <div class="form-group">
        <label asp-for="Phone"></label>
        <input asp-for="Phone" id="Phone" name="Phone" class="easyui-textbox" style="width:100%;height:30px" value="@Model.Phone">
    </div>
    <div class="form-group">
        <label asp-for="CellPhone"></label>
        <input asp-for="CellPhone" id="CellPhone" name="CellPhone" class="easyui-textbox" style="width:100%;height:30px" value="@Model.CellPhone">
    </div>
    <div class="form-group">
        <label asp-for="WorkRegion"></label>
        <input asp-for="WorkRegion" id="WorkRegion" name="WorkRegion" class="easyui-textbox" style="width:100%;height:30px" value="@Model.WorkRegion">
    </div>
    <div class="form-group">
        <label asp-for="Ability"></label>
        <input asp-for="Ability" id="Ability" name="Ability" class="easyui-textbox" style="width:100%;height:30px" value="@Model.Ability">
    </div>
    <div class="form-group">
        <label asp-for="Status"></label>
        <input asp-for="Status" id="Status" name="Status" class="easyui-textbox" style="width:100%;height:30px" value="@Model.Status">
    </div>

    <button type="submit" class="btn btn-primary btn-block mt-4">Edit</button>
</form>